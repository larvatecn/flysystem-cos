{
  "name": "larva/flysystem-cos-v3",
  "description": "This is a Flysystem adapter for the Tencent COS.",
  "keywords": [ "flysystem", "tencent", "cos", "larva" ],
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^7.4 | ^8.0",
    "league/flysystem": "^1.0.0",
    "larva/cos-sdk-v5": "^2.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Larva\\Flysystem\\TencentCos\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
